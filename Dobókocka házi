import random
a1=random.randrange(1,6)
print(a1)
if a1==1:
  print('...')
  print('.*.')
  print('...')
elif a1==2:
  print('...')
  print('*.*')
  print('...')
elif a1==3:
  print('..*')
  print('.*.')
  print('*..')
elif a1==4:
  print('*.*')
  print('...')
  print('*.*')
elif a1==5:
  print('*.*')
  print('.*.')
  print('*.*')
else:
  print('*.*')
  print('*.*')
  print('*.*')
